<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.seeyon.v3x.exchange.domain.EdocSendRecord" 
    table="edoc_exchange_send"
    lazy="true"
>
    <id
        name="id"
        type="long"
        column="id"
        length="20"
    >
        <generator class="assigned" />
    </id>
    <property
        name="subject"
        type="string"
        column="subject"
        not-null="true"
        length="1000"
    />
    <property
        name="docType"
        type="string"
        column="doc_type"
        length="255"
    />
	<property
        name="docMark"
        type="string"
        column="doc_mark"
        length="255"
    />
	<property
        name="secretLevel"
        type="string"
        column="secret_level"
        length="255"
    />
	<property
        name="urgentLevel"
        type="string"
        column="urgent_level"
        length="255"
    />
	<property
        name="sendUnit"
        type="string"
        column="send_unit"
        not-null="true"
        length="255"
    />
	<property
        name="issuer"
        type="string"
        column="issuer"
        length="255"
    />
    <property
        name="issueDate"
        type="date"
        column="issue_date"
        length="10"
    /> 
	<property
        name="copies"
        type="integer"
        column="copies"
        length="11"
    />
    <property
        name="edocId"
        type="long"
        column="edoc_id"
        length="20"
    /> 
	<property
        name="sendUserId"
        type="long"
        column="send_user_id"
        length="20"
    />
	<property
        name="assignType"
        type="int"
        column="assign_type"
        length="4"
    />    
	<property
        name="isBase"
        type="int"
        column="is_base"
        length="4"
    />        
	<property
        name="sendTime"
        type="timestamp"
        column="send_time"
        length="19"
    />			
	<property
        name="exchangeOrgId"
        type="long"
        column="exchange_org_id"
        not-null="true"
        length="20"
    />
    <property
        name="exchangeAccountId"
        type="long"
        column="exchange_account_id"
        length="20"
    />
    <property
        name="exchangeType"
        type="integer"
        column="exchange_type"
        not-null="true"
        length="4"
    />
    <property
        name="createTime"
        type="timestamp"
        column="create_time"
		not-null="true"
        length="19"
    />
    <property
        name="status"
        type="integer"
        column="status"
        not-null="true"
        length="4"
    />
    <property
        name="contentNo"
        type="integer"
        column="content_no"
        length="4"
    />
    <property
        name="sendedTypeIds"
        type="string"
        column="sended_typeids" 
    />
    <property
        name="stepBackInfo"
        type="string"
        column="stepBack_info"
        length="1000"
    />
    <property
        name="isTurnRec"
        type="integer"
        column="is_turn_rec"
        length="1"
    />
    <property name="sendedNames" type="string" column="sended_names" />
     <property
        name="exchangeMode"
        type="integer"
        column="exchange_mode"
        not-null="false"
        length="4"
    />
    <!-- 客开 start -->
    <property
        name="sendedTypeIds_yuewen"
        type="string"
        column="sended_typeids_yuewen" 
    />
    <property
        name="sendedTypeIds_nbfs"
        type="string"
        column="sended_typeids_nbfs" 
    />
    <property
        name="sendedNames_yuewen"
        type="string"
        column="sended_names_yuewen" 
    />
    <property
        name="sendedNames_nbfs"
        type="string"
        column="sended_names_nbfs" 
    />
    <!-- 客开 end -->
	<!-- Associations -->

	<set
        name="sendDetails"
        cascade="none"
        lazy="false"
    >
        <key>
            <column name="send_edoc_id" /> <!-- a foreign key in exchange_send_edoc_detail referencing the primary key of this table. -->
        </key>
        <one-to-many class="com.seeyon.v3x.exchange.domain.EdocSendDetail" />
    </set>
	
</class>
</hibernate-mapping>