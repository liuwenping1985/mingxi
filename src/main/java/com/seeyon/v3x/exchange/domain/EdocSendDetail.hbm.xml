<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.seeyon.v3x.exchange.domain.EdocSendDetail" 
    table="edoc_exchange_send_detail"
    lazy="true"
>
    <id
        name="id"
        type="long"
        column="id"
        length="20"
    >
        <generator class="assigned" />
    </id>
    <property
        name="sendRecordId"
        type="long"
        column="send_edoc_id"
        not-null="false"
        length="20"
    />
    <property
        name="recOrgId"
        type="string"
        column="rec_org_id"
		not-null="true"
        length="255"
    />
	<property
        name="recOrgType"
        type="integer"
        column="rec_org_type"
		not-null="true"
        length="4"
    />
    <property
    	name="recOrgName"
    	type="string"
    	column="rec_org_name"
    	length="255"
    />
	<property
        name="sendType"
        type="integer"
        column="send_type"
		not-null="true"
        length="4"
    />
	<property
        name="content"
        type="string"
        column="content"
        length="255"
    />
	<property
        name="recNo"
        type="string"
        column="rec_no"
        length="255"
    />
	<property
        name="recUserName"
        type="string"
        column="rec_user_name"
        length="255"
    />    
	<property
        name="recTime"
        type="timestamp"
        column="rec_time"
        length="19"
    />	
    <property
        name="status"
        type="integer"
        column="status"
        not-null="true"
        length="4"
    />	
    <property
        name="cuibanNum"
        type="integer"
        column="cuiban_num"
        length="4"
    />  
    <!-- 客开start -->
    <property
    	name="type"
    	type="string"
    	column="type"
    	length="50"
    />
    <!-- 客开end -->
</class>
</hibernate-mapping>