<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="com.seeyon.v3x.bulletin.domain.BulType" table="bul_type">
		<id name="id" column="id" type="long" length="20">
			<generator class="assigned" />
		</id>

		<property name="typeName" column="type_name" type="string" not-null="true" length="50" />
		<property name="usedFlag" column="used_flag" type="boolean" not-null="true" length="1" />
		<property name="description" column="description" type="string" />
		<property name="topCount" column="top_count" type="byte" not-null="true" length="4" />
		<property name="auditFlag" column="audit_flag" type="boolean" not-null="true" length="1" />
		<property name="auditUser" column="audit_user" type="long" not-null="true" length="20" />
		<property name="createDate" column="create_date" type="timestamp" not-null="true" length="19" />
		<property name="createUser" column="create_user" type="long" not-null="true" length="20" />
		<property name="updateDate" column="update_date" type="timestamp" length="19" />
		<property name="updateUser" column="update_user" type="long" length="20" />
		<property name="accountId" column="accountId" type="long" not-null="true" />
		<property name="spaceType" column="spaceType" type="integer" not-null="true" />
		<property name="ext1" column="ext1" type="string" length="50" />
		<property name="ext2" column="ext2" type="string" length="50" />
		<property name="sortNum" column="sort_num" type="integer" not-null="true" />
		<property name="isAuditorModify" column="is_auditor_modify" type="boolean" />
		<property name="printFlag" column="print_flag" type="boolean" length="1" />
		<property name="printDefault" column="print_default" type="boolean" length="1" />
		<!-- 客开start -->
		<property name="typesettingStaff" column="typesetting_staff" not-null="true" type="long" length="20" />
		<property name="typesettingFlag" column="typesetting_flag" not-null="true" type="boolean" length="1" />
		<!-- 客开end -->
		
		<property name="defaultPublish" column="default_publish" type="boolean" length="1" />
		<property name="finalPublish" column="final_publish" type="integer" length="1" />
		<property name="writePermit" column="write_permit" type="boolean" length="1" />
	</class>
</hibernate-mapping>