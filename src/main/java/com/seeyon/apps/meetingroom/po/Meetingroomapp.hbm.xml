<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.seeyon.apps.meetingroom.po.MeetingRoomApp" table="meeting_room_app">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="assigned"></generator>
        </id>
        <property name="roomId" type="java.lang.Long">
            <column name="meetingroomId" not-null="true" />
        </property>
        <property name="perId" type="java.lang.Long">
            <column name="perId" not-null="true" />
        </property>
        <property name="auditingId" type="java.lang.Long">
            <column name="auditing_id" />
        </property>
         <property name="departmentId" type="java.lang.Long">
            <column name="departmentId" not-null="true" />
        </property>
        <property name="startDatetime" type="java.util.Date">
            <column name="startDatetime" length="0" not-null="true" />
        </property>
        <property name="endDatetime" type="java.util.Date">
            <column name="endDatetime" length="0" not-null="true" />
        </property>
        <property name="meetingId" type="java.lang.Long">
            <column name="meetingId" />
        </property>
        <property name="templateId" type="java.lang.Long">
            <column name="template_id" />
        </property>
        <property name="periodicityId" type="java.lang.Long">
            <column name="periodicity_id" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="description" />
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="status" not-null="true" />
        </property>
        <property name="usedStatus" type="java.lang.Integer">
            <column name="used_status" not-null="true" />
        </property>
        <property name="appDatetime" type="java.util.Date">
            <column name="appDatetime" length="0" />
        </property>
         <!--项目  信达资产   公司  kimde  修改人  msg  修改时间    2017-11-09   修改功能   申请会议室页面添加申请人数量 和申请设备  start   -->
        <property name="peopleNumber" type="java.lang.Integer">
            <column name="peopleNumber" length="50" />
        </property>
        <property name="shebei" type="java.lang.String">
            <column name="shebei" length="300" />
        </property>
        
         <property name="meetingType" type="java.lang.String">
            <column name="meeting_Type" length="50" />
        </property>
         <property name="meetingmiaoshu" type="java.lang.String">
            <column name="meeting_miaoshu" length="300" />
        </property>
        <!--项目  信达资产   公司  kimde  修改人  msg  修改时间    2017-11-09   修改功能   申请会议室页面添加申请人数量 和申请设备  end -->
        
    </class>
</hibernate-mapping>