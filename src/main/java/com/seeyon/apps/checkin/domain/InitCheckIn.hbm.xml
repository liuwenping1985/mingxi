<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.seeyon.v3x.plugin.checkin.domain.InitCheckIn"
		table="initcheckin">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		<!-- 用户id -->
		<property name="userId" type="java.lang.String">
			<column name="userId" length="100" />
		</property>
		<!-- 打卡时间 -->
		<property name="checkTime" type="java.sql.Timestamp">
			<column name="checktime" length="100" />
		</property>
		<!-- 状态 -->
		<property name="flag" type="java.lang.String">
			<column name="flag" length="5" />
		</property>
		<!-- 星期 -->
		<property name="week" type="java.lang.String">
			<column name="week" length="100" />
		</property>
		<!-- 请假类型 -->
		<property name="leaveType" type="java.lang.String">
			<column name="leaveType" length="100" />
		</property>
	</class>
</hibernate-mapping>
