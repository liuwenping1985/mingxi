<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.seeyon.apps.taskcenter.po.ProSenderUrl" table="pro_sender_url">
        <id name="id" type="java.lang.Long">
            <column name="Id" />
            <generator class="assigned" />
        </id>
        <property name="code" type="java.lang.String">
            <column name="code" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
        <property name="url" type="text">
            <column name="url"/>
        </property>
        <property name="createTime" type="timestamp">
            <column name="createTime" />
        </property>
        <property name="updateTime" type="timestamp">
            <column name="updateTime"/>
        </property>
        <property name="createUser" type="java.lang.Long">
            <column name="createUser" />
        </property>
        <property name="accountId" type="java.lang.Long">
            <column name="accountId" />
        </property>
        <property name="roleId" type="java.lang.Long">
            <column name="roleId" />
        </property>
    </class>
</hibernate-mapping>
