        <property not-null="false" name="createDate" column="create_date" type="date"/>
        <property not-null="false" name="userId" column="user_id" type="long"/>
        <property not-null="false" length="512" name="deptName" column="dept_name" type="string"/>
        <property not-null="false" length="512" name="deptId" column="dept_id" type="string"/>
        <property not-null="false" name="formRecordId" column="form_record_id" type="long"/>
        <property not-null="false" length="512" name="tableName" column="table_name" type="string"/>
