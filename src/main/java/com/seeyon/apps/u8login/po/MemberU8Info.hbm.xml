<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.u8login.po">
    <!-- 业务管理配置表 -->
    <class name="MemberU8Info" table="member_u8_info">
        <id
                name="id"
                type="long"
                column="id"
                length="20"
        >
        </id>

        <!-- 年 -->
        <property
                name="userCode"
                column="user_code"
                type="string"
                not-null="false"
                length="512"
        />
        <!-- 月 -->
        <property
                name="password"
                column="password"
                type="string"
                not-null="false"
                length="512"
        />


    </class>
</hibernate-mapping>
