<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.u8login.po">
    <!-- 业务管理配置表 -->
    <class name="OAMember" table="org_member">
        <id
                name="id"
                type="long"
                column="id"
                length="20"
        >
        </id>
        <property
                name="accountId"
                column="org_account_id"
                type="long"
                length="20"
        />
        <!-- 月 -->
        <property
                name="code"
                column="code"
                type="string"
                not-null="false"
                length="256"
        />
        <property
                name="isDelete"
                column="is_deleted"
                type="int"
                not-null="false"
        />
        <property
                name="deptId"
                column="org_department_id"
                type="long"
                not-null="false"
                length="20"
        />

    </class>
</hibernate-mapping>
