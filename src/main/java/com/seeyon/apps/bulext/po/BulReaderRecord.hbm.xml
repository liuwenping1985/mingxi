<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.bulext.po">
	<class name="BulReaderRecord" lazy="false" table="bul_read_record">
		<id name="id" type="long" column="id" length="20">
			<generator class="assigned" />
		</id>
		<property name="bulId" not-null="false" length="20" type="long" column="bul_id" />
		<property name="memberId" not-null="false" length="20" type="long" column="member_id"/>
		<property name="readDate" not-null="false" type="timestamp" column="read_date"/>
	</class>
</hibernate-mapping>