<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.edocduban.po">
    <class name="EdocDubanPo" lazy="false" table="edoc_duban">
        <id name="id" type="long" column="id" length="20">
            <generator class="assigned"/>
        </id>
        <property name="createTime" type="timestamp" column="create_time"  length="19"/>
        <property name="updateTime" type="timestamp" column="update_time"  length="19"/>
        <property name="summaryId" type="long" column="summary_id" not-null="true" length="20"/>
        <property name="state" type="integer" column="state" not-null="true" length="4"/>
        <property name="remark1" type="string" column="remark1" length="1000"/>
    </class>
</hibernate-mapping>
