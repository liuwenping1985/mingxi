<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.edocduban.po">
    <class name="EdocDubanSendPo" lazy="false" table="edoc_duban_send">
        <id name="id" type="long" column="id" length="20">
            <generator class="assigned"/>
        </id>
        <property name="createDate" type="timestamp" column="create_date"  length="19"/>
        <property name="summaryId" type="long" column="summary_id" not-null="true" length="20"/>
        <property name="formId" type="long" column="form_Id" not-null="true" length="4"/>
        <property name="remark" type="string" column="remark" length="1000"/>
    </class>
</hibernate-mapping>
