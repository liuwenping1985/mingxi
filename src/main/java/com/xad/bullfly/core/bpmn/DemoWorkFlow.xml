<?xml version="1.0" encoding="utf-8" ?>
<xad-bpmn>
    <process name="流程demo" id="processOne">
    <flow-sequences>
        <!-- injection 表示是否给reference值，START表示开始 流程创建的时候处于这个状态-->
        <sequence-node  type="START" mode="system_node" member-rule="NONE" injection="NONE"></sequence-node>
        <sequence rule="NEXT"></sequence>
        <sequence-node  name="督办员发起者填东西" type="COMMON" member-rule="SET" member-mode="single"  injection="NONE" output="" ></sequence-node>
        <sequence rule="NEXT"></sequence>
        <sequence-node  name="督办员领导审批" type="APPROVAL" member-rule="SET" member-mode="single"  injection="NONE" output="" ></sequence-node>
        <sequence rule="NEXT"></sequence>
        <sequence-node  name="承办部门签收" type="APPROVAL" member-rule="SET" member-mode="single"  injection="NONE" output="" ></sequence-node>
        <sequence rule="NEXT"></sequence>
        <sequence-node  name="协办部门签收" type="APPROVAL" member-rule="SET" member-mode="multi"  injection="NONE" output="" ></sequence-node>
        <sequence rule="NEXT"></sequence>
        <sequence-node  name="督办员完成" type="APPROVAL" member-rule="SET" member-mode="single"  injection="NONE" output="" ></sequence-node>
        <sequence rule="NEXT"></sequence>
        <sequence-node type="END" member-rule="END" member-mode="single"  injection="NONE" output="" ></sequence-node>

    </flow-sequences>
        
        <dataset>

        </dataset>
    <rules>
        <rule name="NONE">

        </rule>
        <rule name="SET">

        </rule>
        <rule name="RULE_LEADER">
            <executor type="java">@xadWorkFlowRuleProvider.getUserLeader(execution,user)</executor>
        </rule>
        <rule name="RULE_SUPERVISOR">
            <executor type="java">@xadWorkFlowRuleProvider.getSupervisor(execution,user)</executor>
        </rule>
        <rule name="RULE_USER_LIST">
            <executor type="java">@xadWorkFlowRuleProvider.getUserList(execution,user)</executor>
        </rule>
        <rule name="RULE_BINDDATA">
            <executor type="java">@xadWorkFlowRuleProvider.getDubanTask(execution,user)</executor>
        </rule>
    </rules>

    </process>
</xad-bpmn>