        </sequence>
        <sequence id="seq3" type="COMMON" link="NEXT-seq4">
            <sequence-node  name="督办员领导审批" type="APPROVAL" member-rule="RULE_LEADER" member-mode="single"  injection="NONE" output="" ></sequence-node>
