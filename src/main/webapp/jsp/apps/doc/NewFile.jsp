    /**
 *  æ ¹æ®å¹³ååè¿åææ¡£çå¯¹åºçæçº§classå±æ§å¼
 *  @param avgScore ææ¡£å¹³åå[Constants.SCORE_MIN=0,Constants.SCORE_MAX=5]
 *  @returns è¿åææ¡£çå¯¹åºçæçº§classå±æ§å¼
 */
function fnGetStarLevelClass(avgScore) {
    // å¦æä¼ å¥å¹³ååä¸æ¯æææ°å­
    if(avgScore == null || isNaN(avgScore)) {
        return 'stars0';
    }
    // å¦æå¹³ååèå´æ æ
    var avg = parseFloat(avgScore);
    if(avg > 5 || avg < 0) {
        return 'stars0';
    }
    var level = 'stars';
    var floatPart = avg - parseInt(avg);
    if(floatPart < 0.25) {
        level += parseInt(avg);
    } else if(floatPart > 0.75) {
        level += parseInt(avg) + 1;
    } else {
        level += parseInt(avg).toString() + '_5';
    }
    return level;
}

/**
 *  è®¡ç®ç»å®æ¶é´å°å½åæ¶é´çæ¶é´é´é
 *  @param strDate æ¥ææ¶é´æ ¼å¼å­ç¬¦ä¸² æ ¼å¼å¦ï¼(2012/09/19 or 2012-09-19)[ (18:25:35 or 18:25)]
 *  @returns è¿åè·ç¦»å½åæ¶é´é´é,å¯¹äºæ æ³è§£æçæ ¼å¼åæ ·è¿å
 */
function fnEvalTimeInterval(strDate) {
    var strDateFormat = strDate.replace('-', '/');
    var myDate = new Date(strDateFormat);
    if(isNaN(myDate)) {
        return strDate;
    }
    var timeInterval = new Date().getTime() - myDate.getTime();
    var seconds = timeInterval / 1000;
    if (seconds < 0) {
        return strDate;
    }  else {
        var strInterval = '';
        if (seconds < 60) {
            strInterval = Math.floor(seconds) + "秒";
        }  else {
            var minus = seconds/60;
            if(minus < 60) {
                strInterval = Math.floor(minus) + "分";
            } else {
                var hours = minus/60;
                if (hours < 24) {
                    strInterval = Math.floor(hours) + "小时";
                } else {
                    var days = hours/24;
                    if (days < 30) {
                        strInterval = Math.floor(days) + "天";
                    } else {
                        var months = days/30;
                        if (months < 12) {
                            strInterval = Math.floor(months) + "个月";
                        } else {
                            var years = months / 12;
                            strInterval = Math.floor(years) + "年";
                        }
                    }
                }
            }
        }
        return strInterval + "前";
    }
}

