<?xml version="1.0" encoding="UTF-8"?>
<productFeature>
	<!-- 命名规则：模块为一层， 功能标志为子层，如下：-->
    <system>
        <!-- 
        a6(0),              // A6版
        enterprise(1),      // 企业版
        entgroup(2),        // 企业集团版
        government(3),      // 政务版
        governmentgroup(4), // 政务多组织版
        ufidanc(5);         // UFIDA-NC协同套件
		ufidau8(6);         // UFIDA-U8协同套件
		a6-s(7);         // A6-S版
        -->
        <ProductId>3</ProductId>
        
        <EditionSuffix>.GOV</EditionSuffix>
        
        <allowUnitChangePriv>true</allowUnitChangePriv>
        <logCode>F13_unitLog</logCode>
        <geniusFolder>G6</geniusFolder>
        <help>3</help>
    </system>
	<col>
		<newAuto>true</newAuto>
	</col>
	<org>
		<isGroupVer>false</isGroupVer>
		<GroupPost>false</GroupPost>
		<GroupLevel>false</GroupLevel>
	</org>
	<doc>
		<rssFlag>true</rssFlag>
		<blogFlag>true</blogFlag>
		<docOpenFlag>false</docOpenFlag>
		<forumFlag>false</forumFlag>
		<collectFlag>false</collectFlag>
		<recommendFlag>false</recommendFlag>
		<openToZoneFlag>false</openToZoneFlag>
	</doc>
	<portal>
       <performanceFlag>true</performanceFlag>
       <themSpaceFlag>true</themSpaceFlag>
       <porletSelectorFlag>G6</porletSelectorFlag>
       <favicon></favicon>
	   <about></about>
    </portal>
    <edoc>
    	<!-- 是否有发文种类功能 -->
    	<hasEdocCategory>true</hasEdocCategory>
    	<!-- 收文是否有政务公文登记环节 -->
    	<hasEdocRegister>true</hasEdocRegister>
    	<!-- 是否G6，临时使用，公文用到的地方太多，后续清理 -->
    	<isG6>true</isG6>
    </edoc>
</productFeature>