<?xml version="1.0" encoding="UTF-8"?>
<portlets>
	<portlet>
		<!-- 唯一标识，用于权限判断及Portlet数据更新 （个人行为绩效）-->
		<portletId>personalbehavioranalysis</portletId>
		<!-- 需要进行权限判断时使用的资源Id -->
		<resourceCode>F08_personalbehavioranalysis</resourceCode>
		<!-- 需要进行权限判断时使用的插件Id -->
		<pluginId>behavioranalysis</pluginId>
		<!-- Portlet名称,可以为空 -->
		<portletName>behavioranalysis.portlet.personalbehavioranalysis</portletName>
		<!-- Portlet显示名称,不能为空,预制数据需要国际化 -->
		<displayName>behavioranalysis.portlet.personalbehavioranalysis</displayName>
		<!-- Portlet所属空间类别 -->
		<spaceTypes>personal,personal_custom,leader,outer,custom,department,corporation,public_custom,group,public_custom_group,cooperation_work,objective_manage,edoc_manage,meeting_manage,performance_analysis,form_application,related_project_space,m3mobile,weixinmobile</spaceTypes>
		<!-- Portlet分类 -->
		<category>performanceAnalysis</category>
		<!-- Portlet点击响应地址 -->
		<portletUrl><![CDATA[/behavioranalysis.do?method=personalindex]]></portletUrl>
		<!-- Portlet点击响应地址类型,space:空间地址,workspace:系统工作区地址,open:系统新窗口地址,link:外部地址 -->
		<portletUrlType>open</portletUrlType>
		<!-- Portlet大小,0:中,1:大 -->
		<size>0</size>
		<!-- Portlet排序 -->
		<order>8</order>
		<!-- Portlet展示图片 -->
		<imageLayouts>
			<imageLayout>
				<!-- 图片路径 -->
				<imageUrl><![CDATA[d_collaborationcube.png]]></imageUrl>
				<!-- 图片名称 -->
				<imageName>behavioranalysis.portlet.orgbehavioranalysis</imageName>
				<!-- 图片内容标题 -->
				<imageTitle></imageTitle>
				<!-- 图片内容摘要 -->
				<summary></summary>
			</imageLayout>
		</imageLayouts>
	</portlet>
	
	<portlet>
		<portletId>orgbehavioranalysis</portletId>
		<resourceCode>F08_orgbehavioranalysis</resourceCode>
		<pluginId>behavioranalysis</pluginId>
		<portletName>behavioranalysis.portlet.orgbehavioranalysis</portletName>
		<displayName>behavioranalysis.portlet.orgbehavioranalysis</displayName>
		<category>performanceAnalysis</category>
		<portletUrl><![CDATA[/behavioranalysis.do?method=orgindex]]></portletUrl>
		<portletUrlType>open</portletUrlType>
		<size>0</size>
		<order>9</order>
		<imageLayouts>
			<imageLayout>
				<imageUrl><![CDATA[d_performancequery.png]]></imageUrl>
				<imageName>behavioranalysis.portlet.personalbehavioranalysis</imageName>
				<imageTitle></imageTitle>
				<summary></summary>
			</imageLayout>
		</imageLayouts>
	</portlet>
</portlets>
