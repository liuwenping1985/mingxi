								<property name="name" value="columnsStyle" />
								<property name="subject" value="cannel.style.label" />
								<property name="onChange" value="changeDefaultValue4LinkSystem" />
								<property name="changeValue" value="listCountD=32 pictureCountD=10" />
								<property name="valueType" value="0" />
								<property name="defaultValue" value="picture" />
								<property name="valueRanges">
									<list>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="imagenews.list.label" />
											<property name="value" value="list" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="imagenews.head.label" />
