<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">
	
	<!-- 考勤签到(地图)栏目 -->
	<bean id="lbsMapChartSection" class="com.seeyon.apps.lbs.section.LbsMapChartSection" init-method="init">
		<property name="sortId" value="11"/>
		<property name="sectionType" value="common"/>
		<property name="spaceTypes" value="custom,personal,personal_custom,leader,cooperation_work,objective_manage,edoc_manage,meeting_manage,performance_analysis,form_application,outer,department"/>
		<property name="properties">
			<list>
				<bean class="com.seeyon.ctp.portal.section.SectionPropertyImpl">
					<property name="reference">
						<list>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnsName" />
								<property name="subject" value="cannel.name.label" />
								<property name="valueType" value="2" />
								<property name="defaultValue" value="lbs.map.label" />
								<property name="validate" value="notNull"></property>
								<property name="validateValue" value="notNull=true maxLength=2000"></property>
							</bean>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnsHeight" />
								<property name="subject" value="lbs.height.label" />
								<property name="valueType" value="2" />
								<property name="defaultValue" value="410" />
								<property name="validate" value="isInteger"></property>
								<property name="validateValue" value="notNull=true min=410 max=768"></property>
							</bean>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
	
	<!-- 考勤签到(列表)栏目 -->
	<bean id="lbsListSection" class="com.seeyon.apps.lbs.section.LbsListSection" init-method="init">
		<property name="mLbsRecordManager" ref="mLbsRecordManager"/>
		<property name="sortId" value="11"/>
		<property name="sectionType" value="common"/>
		<property name="spaceTypes" value="custom,personal,personal_custom,leader,cooperation_work,objective_manage,edoc_manage,meeting_manage,performance_analysis,form_application,outer,department"/>
		<property name="properties">
			<list>
				<bean class="com.seeyon.ctp.portal.section.SectionPropertyImpl">
					<property name="reference">
						<list>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnsName" />
								<property name="subject" value="project.collaboration.subject.label" />
								<property name="valueType" value="2" />
								<property name="defaultValue" value="lbs.list.label" />
							</bean>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnsStyle" />
								<property name="subject" value="project.collaboration.style.name" />
								<property name="defaultValue" value="1" />
								<property name="valueType" value="0" />
								<property name="valueRanges">
									<list>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="project.collaboration.style.oneList" />
											<property name="value" value="1" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="project.collaboration.style.twoList" />
											<property name="value" value="2" />
										</bean>
									</list>
								</property>
							</bean>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnsCount" />
								<property name="subject" value="project.collaboration.showline.label" />
								<property name="valueType" value="2" />
								<property name="defaultValue" value="10" />
								<property name="validate" value="isInteger"></property>
								<property name="validateValue" value="isInteger=true max=50 min=10"></property>
							</bean>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnProperty" />
								<property name="subject" value="project.collaboration.show.label" />
								<property name="defaultValue" value="userName,dptName,createDate,lbsAddr" />
								<property name="valueType" value="1" />
								<property name="valueRanges">
									<list>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="lbs.name.label" />
											<property name="value" value="userName" />
											<property name="readOnly" value="true"/>
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="lbs.department.label" />
											<property name="value" value="dptName" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="lbs.time.label" />
											<property name="value" value="createDate" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="lbs.addres.label" />
											<property name="value" value="lbsAddr" />
										</bean>
									</list>
								</property>
							</bean>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
</beans>