	<!-- 项目日程事件 -->
	<bean class="com.seeyon.ctp.portal.section.ProjectCalEventSection" id="projectCalEventSection">
		<property name="sortId" value="108"/>
		<property name="sectionType" value="common,timeManagement"/>
		<property name="spaceTypes" value="related_project_space"/>
		<property name="properties">
			<list>
				<bean class="com.seeyon.ctp.portal.section.SectionPropertyImpl">
					<property name="reference">
						<list>
							<!-- 栏目名称 -->
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnsName" />
								<property name="subject" value="cannel.name.label" />
								<property name="valueType" value="2" />
								<property name="defaultValue" value="calendar.share.event.project" />
								<property name="validateValue" value="notNull=true maxLength=2000"></property>
							</bean>
							<!-- 栏目显示行数-->
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnsCount" />
								<property name="subject" value="collaboration.cannel.data.count" />
								<property name="valueType" value="2" />
								<property name="defaultValue" value="8" />
								<property name="validate" value="isInteger"></property>
								<property name="validateValue" value="isInteger=true max=50 min=1"></property>
							</bean>
							<!-- 栏目显示字段, 0-标题,1-创建时间,2-创建人,3-类型(此字段废弃),4-状态 -->
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnProperty" />
								<property name="subject" value="cannel.display.column.label" />
								<property name="valueType" value="1" />
								<property name="defaultValue" value="title,createTime,creator,state" />
								<property name="valueRanges">
									<list>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="calendar.project.event.subject" />
											<property name="value" value="title" />
											<property name="readOnly" value="true" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="calendar.project.event.create.time" />
											<property name="value" value="createTime" />
										</bean>
										<bean class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="calendar.project.event.create.people" />
											<property name="value" value="creator" />
										</bean>
										<!--此字段废弃 
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="calendar.project.event.type" />
											<property name="value" value="type" />
										</bean>
										 -->
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="calendar.project.event.state" />
											<property name="value" value="state" />
										</bean>
									</list>
								</property>
							</bean>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
	
	
