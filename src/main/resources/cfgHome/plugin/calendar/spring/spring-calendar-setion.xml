                                        </bean>
                                    </list>
                                </property>
                            </bean>
                            
                            <bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
                                <property name="name" value="columnstyle" />
                                <property name="subject" value="cannel.style.label" />
                                <property name="onChange" value="leaderScheduleChangeByStyle" />
                                <property name="changeValue" value="listCountD=32 pictureCountD=10" />
                                <property name="valueType" value="0" />
                                <property name="defaultValue" value="list" />
                                <property name="valueRanges">
                                    <list>
                                        <bean class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
                                            <property name="subject" value="imagenews.list.label" />
                                            <property name="value" value="list" />
                                        </bean>
                                        <bean class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
                                            <property name="subject" value="imagenews.head.label" />
                                            <property name="value" value="picture" />
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                            
                            <bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
                                <property name="name" value="columnscount" />
                                <property name="subject" value="collaboration.cannel.data.count" />
                                <property name="valueType" value="2" />
                                <property name="defaultValue" value="8" />
                                <property name="validate" value="isInteger" />
                                <property name="validateValue" value="max=50 min=1 maxLength=2" />
                            </bean>
                        
                            <!-- 多选框 -->
                            <bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
                                <property name="name" value="columnproperty" />
                                <property name="subject" value="cannel.display.column.label" />
                                <property name="valueType" value="1" />
                                <property name="defaultValue" value="0,1,2,5" />
                                <property name="valueRanges">
                                    <list>
                                        <bean
                                            class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
                                            <property name="subject" value="online.date.label" />
                                            <property name="value" value="0" />
                                        </bean>
                                        <bean
                                            class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
                                            <property name="subject" value="common.member.name" />
                                            <property name="value" value="1" />
                                            <property name="readOnly" value="true" />
                                        </bean>
                                        <bean
                                            class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
                                            <property name="subject" value="common.subject.label" />
                                            <property name="value" value="2" />
                                        </bean>
                                        <bean
                                            class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
                                            <property name="subject" value="org.account.label" />
                                            <property name="value" value="3" />
                                        </bean>
                                        <bean
                                            class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
                                            <property name="subject" value="org.department.label" />
                                            <property name="value" value="4" />
                                        </bean>
                                        <bean
                                            class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
                                            <property name="subject" value="calendar.level.label" />
                                            <property name="value" value="5" />
                                        </bean>
                                    </list>
                                </property>
                            </bean>                    
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>
    
