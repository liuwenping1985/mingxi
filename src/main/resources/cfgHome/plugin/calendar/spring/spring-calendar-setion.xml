            <map>
                <entry key="2" value="1" />
                <entry key="3" value="2" />
                <entry key="4" value="3" />
                <entry key="5" value="4" />
                <entry key="8" value="6" />
                <entry key="10" value="8" />
            </map>
        </property>
        <property name="newLine2Column1">
            <map>
                <entry key="2" value="2" />
                <entry key="3" value="3" />
                <entry key="4" value="4" />
                <entry key="5" value="5" />
                <entry key="8" value="8" />
                <entry key="10" value="10" />
            </map>
        </property>
        <property name="properties">
            <list>
                <bean class="com.seeyon.ctp.portal.section.SectionPropertyImpl">
                    <property name="reference">
                        <list>
                            <!-- 栏目标题 -->
                            <bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
                                <property name="name" value="columnsName" />
