    <bean name="/publicResource.do" class="com.seeyon.v3x.pubResource.controller.PublicResourceController"/>
    
    <bean id="meetingAssDoc" class="com.seeyon.ctp.common.assdoc.AssdocDefinition">
        <property name="appId" value="6" />
