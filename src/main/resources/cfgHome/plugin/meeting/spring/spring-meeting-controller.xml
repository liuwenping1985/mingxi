    </bean>
    
    <bean name="/meetingDetail.do" class="com.seeyon.v3x.meeting.controller.MeetingDetailController"></bean>
    
    <bean name="/mtTemplate.do" class="com.seeyon.v3x.meeting.controller.MtTemplateController">
        <property name="metadataManager" ref="v3xmetadataManager"/>
    </bean>
    <bean name="/mtContentTemplate.do" class="com.seeyon.v3x.meeting.controller.MtContentTemplateController"></bean>
    
    <bean name="/mtAppMeetingController.do" class="com.seeyon.v3x.meeting.controller.MtAppMeetingController">
