<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">

	<bean name="/meetingNavigation.do" class="com.seeyon.v3x.meeting.controller.MeetingNavigationController"></bean>

    <bean name="/meeting.do" class="com.seeyon.v3x.meeting.controller.MeetingController"></bean>
     
    <bean name="/mtMeeting.do" class="com.seeyon.v3x.meeting.controller.MtMeetingController">
