								<property name="validate" value="isInteger" />
								<property name="validateValue" value="max=768 min=208" />
								<property name="defaultValue" value="208" />
							</bean>				
							 <bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
                                <property name="name" value="showContent" />
