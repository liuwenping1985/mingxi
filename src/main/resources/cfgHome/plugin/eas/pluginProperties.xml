<?xml version="1.0" encoding="utf-8"?>
<ctpConfig>
	<eas>
		<enabled mark="{option,0,1} {VE}" desc="是否启用EAS插件 0--不启用  1--启用">0</enabled>
		<db>
			<driverClassName
				mark="{option,oracle.jdbc.driver.OracleDriver,net.sourceforge.jtds.jdbc.Driver} {VE}"
				desc="EAS数据库连接，JDBC驱动ClassName">oracle.jdbc.driver.OracleDriver</driverClassName>
			<hibernateDialect
				mark="{option,org.hibernate.dialect.OracleDialect,org.hibernate.dialect.SQLServerDialect} {VE}"
				desc="EAS数据库连接，Hibernate Dialect">org.hibernate.dialect.OracleDialect</hibernateDialect>
			<url mark="{VE}"
				desc="EAS数据库连接; oracle URL:jdbc:oracle:thin:@[DATABASE IP ADDRESS]:[PORT]:[SID],SQLServer URL:jdbc:jtds:sqlserver://[DATABASE IP ADDRESS]:[PORT]/[DATABASE NAME]" >jdbc:oracle:thin:@127.0.0.1:1521:eas</url>
			<username mark="{VE}" desc="EAS数据库连接，用户名">TEST</username>
			<password mark="{password} {VE}" desc="EAS数据库连接，密码">123456</password>
		</db>
	</eas>
</ctpConfig>