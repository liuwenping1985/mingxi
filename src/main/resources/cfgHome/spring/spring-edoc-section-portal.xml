<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName" default-lazy-init="false">

	<!-- 定义公文管理栏目选择器 -->
    <bean id="edocSelector" class="com.seeyon.ctp.portal.section.EdocSectionSelector">
        <property name="sectionType" value="edoc"></property>
    </bean>
    
    <bean id="edocReturnSection" class="com.seeyon.ctp.portal.section.EdocReturnSection" init-method="init">
		<property name="sectionType" value="edoc" />
		<property name="spaceTypes" value="personal,personal_custom,leader,outer,custom,cooperation_work,objective_manage,edoc_manage,meeting_manage" />
		<property name="properties">
			<list>
				<bean class="com.seeyon.ctp.portal.section.SectionPropertyImpl">
					<property name="reference">
						<list>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnsName" />
								<property name="subject" value="cannel.name.label" />
								<property name="valueType" value="2" />
								<property name="defaultValue" value="edoc.return.title" />
							</bean>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnstyle" />
								<property name="subject" value="cannel.style.label" />
								<property name="valueType" value="0" />
								<property name="defaultValue" value="0" />
								<property name="valueRanges">
									<list>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="collaboration.orderList.label" />
											<property name="value" value="0" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="collaboration.doubleList.label" />
											<property name="value" value="1" />
										</bean>
									</list>
								</property>
							</bean>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnscount" />
								<property name="subject" value="collaboration.cannel.data.count" />
								<property name="valueType" value="2" />
								<property name="defaultValue" value="8" />
								<property name="validate" value="isInteger" />
                                <property name="validateValue" value="max=50 min=1" /> 
							</bean>
							<bean class="com.seeyon.ctp.portal.section.SectionReferenceImpl">
								<property name="name" value="columnproperty" />
								<property name="subject" value="cannel.display.column.label" />
								<property name="valueType" value="1" />
								<property name="defaultValue" value="0,2,3" />
								<property name="valueRanges">
									<list>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="common.subject.label" />
											<property name="value" value="0" />
											<property name="readOnly" value="true" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="edoc.supervise.startdate" />
											<property name="value" value="1" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="exchange.edoc.huituishijian" />
											<property name="value" value="2" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="exchange.edoc.huituiren" />
											<property name="value" value="3" />
										</bean>
										<bean
											class="com.seeyon.ctp.portal.section.SectionReferenceValueRangeImpl">
											<property name="subject" value="edoc.supervise.sender" />
											<property name="value" value="4" />
										</bean>
									</list>
								</property>
							</bean>						
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
    
	
