<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">
	<!--DEE扩展控件设置控制器 -->
    <bean name="/dee/deeDesign.do" class="com.seeyon.ctp.form.dee.design.DeeDesignController"></bean>
    <!--DEE前台调用展现控制器-->
    <bean name="/dee/deeDataDesign.do" class="com.seeyon.ctp.form.dee.design.DeeDataController"></bean>
    <!--DEE前台调用触发控制器-->
    <bean name="/dee/deeTrigger.do" class="com.seeyon.ctp.form.dee.trigger.DEETriggerDesignController">
        <property name="deeTriggerDesignManager" ref="deeTriggerDesignManager"/>
    </bean>
</beans>